{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "methods": [
        "get"
      ],
      "route": "report/users"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "table",
      "name": "userTable",
      "tableName": "users",
      "take": 50,
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ]
}