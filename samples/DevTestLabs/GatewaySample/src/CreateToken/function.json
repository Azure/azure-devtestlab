{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "methods": [
        "get"
      ],
      "route": "host/{host}/port/{port}"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "queue",
      "name": "trackTokenQueue",
      "queueName": "track-token",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}