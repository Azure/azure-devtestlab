{
  "$schema": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/schemas/2015-01-01/dtlArtifacts.json",
  "title": "Fiddler4",
  "description": "Installs Fiddler4 using the Chocolatey package manager",
  "tags": [
    "Windows",
    "Fiddler"
  ],
  "iconUri": "https://cdn.rawgit.com/chocolatey/chocolatey-coreteampackages/e4a49519947c3cff55c17a0b08266c56b0613e64/icons/fiddler.svg",
  "targetOsType": "Windows",
  "parameters": {
    "installUsername": {
      "type": "string",
      "displayName": "Install User Name",
      "description": "User with admin rights to install Fiddler4 onto the machine"
    },
    "installPassword": {
      "type": "securestring",
      "displayName": "Install Password",
      "description": "Password for the install user"
    }
  },
  "runCommand": {
    "commandToExecute": "[concat('powershell.exe -File startChocolatey.ps1', ' -RawPackagesList ', 'fiddler4', ' -username ',  parameters('installUsername'), ' -password ', parameters('installPassword'))]"
  }
}