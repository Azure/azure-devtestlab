<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- ========================================== -->
    <!-- These values control authentication to the account -->
    <!-- ========================================== -->    
    <add key="ida:ClientId" value=""/> <!-- Application ID -->
    <add key="TenantId" value=""/> <!-- AD Directory ID -->
    <add key="SubscriptionId" value=""/>
    <add key="AuthTokenOverride" value=""/> <!-- Overrides the above values and allows directly specifying an auth token. -->
    <!-- ========================================== -->
    <!-- Test-specific values -->
    <!-- ========================================== -->    
    <add key="ResourceGroupName" value=""/>
    <add key="LabAccountName" value=""/>
    <add key="LabName" value=""/>
    <add key="UserName" value="NoUserName"/>
    <add key="OutputPath" value="D:\\vnet"/>
    <!-- ========================================== -->
    <!-- You probably won't need to modify these -->
    <!-- ========================================== -->
    <add key="ArmResourceId" value="https://management.core.windows.net/"/>
    <add key="Authority" value="https://login.windows.net/{0}"/>
    <add key="RedirectUri" value="https://dtlclient"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
